	<parameter name="ID_LIQUIDACION" class="java.lang.Integer"/>
	<parameter name="ID_PERSONA" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT C."NOMBRE",
	TC."SENTIDO",
	TC."MONTO",
	L."DESCRIPCION",
	P."NOMBRE",
	P."APELLIDO_PATERNO"
FROM "TABULADO" T
	JOIN "NOU" NOU ON 
	 NOU."ID" = T."ID_NOU" 
	JOIN "PERSONA" P ON 
	 P."ID" = NOU."ID_PERSONA" 
	JOIN "LIQUIDACION" L ON 
	 L."ID" = T."ID_LIQUIDACION" 
	JOIN "TABULADO_CONCEPTO" TC ON 
	 TC."ID_TABULADO" = T."ID" 
	JOIN "CONCEPTO" C ON 
	 C."ID" = TC."ID_CONCEPTO" 
WHERE 
 	L.ID = $P{ID_LIQUIDACION}
  	AND P.ID = $P{ID_PERSONA}]]></query>
